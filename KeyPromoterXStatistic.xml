        <Item>
          <IdeaActionID>FindBugs.PreviewSelectBugInstance</IdeaActionID>
          <ShortCut>'Alt+Q' or 'Alt+Button1 Click'</ShortCut>
          <Description>Preview the current selected bug.</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>